{
  "manifest_version": 3,
  "name": "blindr",
  "description": "An extension that prevents you from acknowledging what you'd rather not",
  "version": "1.1",
  "icons": {
    "16": "public/logo.png",
    "32": "public/logo.png",
    "48": "public/logo.png",
    "128": "public/logo.png"
  },
  "permissions": [ "activeTab", "scripting" ],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "action": {
    "default_icon": {
      "16": "public/logo.png",
      "32": "public/logo.png",
      "48": "public/logo.png",
      "128": "public/logo.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [ "public/baby.gif"],
      "matches": ["<all_urls>"]
    }
  ]
}